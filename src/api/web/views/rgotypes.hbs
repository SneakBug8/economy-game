<h2>Типы добычи</h2>
<table class="table">
    <thead>
    <th>ID</th>
    <th>Добыча</th>
    <th>Регион</th>
    <th>Добывает</th>
    <th>Рабочих</th>
    <th>Построено</th>
    <th>Ресурсы для строительства</th>
    </thead>
{{#each data}}
    <tr>
<td>{{this.id}}</td>
<td>{{this.name}}</td>
<td>{{this.market}}</td>
<td>{{this.makes}}</td>
<td>{{this.workers}}</td>
<td>{{this.already}} / {{this.maxamount}}</td>
<td>{{this.resources}}</td>
    </tr>

{{/each}}

</table>