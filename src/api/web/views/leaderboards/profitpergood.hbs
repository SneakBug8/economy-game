<h2>Прибыль с товара</h2>
<table class="table">
    <thead>
    <th>Игрок</th>
    <th>Прибыль</th>
    <th>Куплено</th>
    <th>Продано</th>
    </thead>
{{#each data}}
    <tr>
<td>{{this.player.username}}</td>
<td>{{this.profit}}</td>
<td>{{this.bought}}</td>
<td>{{this.sold}}</td>
    </tr>
{{/each}}
</table>