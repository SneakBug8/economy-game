<h1>Сводка</h1>
<table class="table-striped">
    <thead>
    <th style="width: 200px;">Дата</th>
    <th style="width: 100px;">Ход</th>
    <th>Текст</th>
    </thead>
{{#each logs}}
    <tr>
<td>{{this.date}}</td>
<td>{{this.turnId}}</td>
<td><b>{{this.text}}</b></td>
    </tr>
{{/each}}

</table>