{
  "name": "dominions",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "tsc && node bin/main.js",
    "start:prod": "node bin/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "build": "tsc",
    "test": "mocha -r ts-node/register src/**/*.spec.ts -r src/moduleloader.ts"
  },
  "dependencies": {
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "app-module-path": "^2.2.0",
    "express-session": "^1.17.1",
    "node": "^10.20.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "sqlite": "^4.0.15",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@nestjs/testing": "^6.0.0",
    "@types/app-module-path": "^2.2.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^10.12.18",
    "@types/sqlite3": "^3.1.6",
    "@types/supertest": "^2.0.7",
    "babel-jest": "^26.5.2",
    "concurrently": "^4.1.0",
    "jest": "^26.5.3",
    "mocha": "^8.1.3",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
